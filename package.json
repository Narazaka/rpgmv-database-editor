{
  "name": "rpgmv-skill-editor",
  "version": "1.0.0",
  "description": "RPGツクールMVスキルエディタ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "npm run dist:ts && npm run dist:static",
    "watch": "parallelshell \"npm run watch:ts\" \"npm run watch:static\" \"npm run watch:view\"",
    "dist:ts": "tsc",
    "watch:ts": "tsc -w",
    "dist:static": "shx mkdir -p dist && shx cp src/index.html \"src/*.css\" dist",
    "watch:static": "npm run dist:static && chokidar src/index.html \"src/*.css\" -c \"npm run dist:static\"",
    "view": "electron dist",
    "watch:view": "cross-env ELECTRON_RELOAD=1 electron dist",
    "lint": "tslint -p ."
  },
  "author": "narazaka",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "cross-env": "^5.1.5",
    "electron": "^2.0.0",
    "electron-packager": "^12.1.0",
    "electron-reload": "^1.2.2",
    "parallelshell": "^3.0.2",
    "shx": "^0.2.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@types/react": "^16.0.40",
    "@types/react-dnd": "^2.0.36",
    "@types/react-dnd-html5-backend": "^2.1.9",
    "@types/react-dom": "^16.0.4",
    "@types/react-jsonschema-form": "^1.0.3",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "moment": "^2.21.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-table": "^4.3.1",
    "react-bootstrap-typeahead": "^3.1.3",
    "react-day-picker": "^7.1.1",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.2.0",
    "react-jsonschema-form": "^1.0.1",
    "react-jsonschema-form-extras": "^0.9.9",
    "react-jsonschema-form-layout-grid": "^2.1.0",
    "react-rte": "^0.16.0"
  }
}
